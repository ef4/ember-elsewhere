{{#if initialized}}
  {{#if hasBlock}}
    {{yield (get (get service.actives name) 'lastObject.component') }}
  {{else}}
    {{#with (get (get service.actives name) 'lastObject.component') as |c|}}
      {{component c params=(get (get service.actives name) 'lastObject.params')}}
    {{/with}}
  {{/if}}
{{/if}}
