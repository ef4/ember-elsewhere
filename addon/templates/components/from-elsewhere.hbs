{{#if initialized}}
  {{#let (get service.actives name) as |active| }}
    {{#if hasBlock}}
      {{yield active.lastObject.component active.lastObject.outsideParams}}
    {{else}}
      {{#with active.lastObject.component as |c|}}
        {{component c outsideParams=active.lastObject.outsideParams}}
      {{/with}}
    {{/if}}
  {{/let}}
{{/if}}
