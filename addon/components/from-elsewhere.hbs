{{#if this.initialized}}
  {{#let (get this.service.actives this.name) as |active| }}
    {{#if (has-block)}}
      {{yield active.lastObject.component active.lastObject.outsideParams}}
    {{else}}
      {{#let active.lastObject.component as |c|}}
        {{component (ensure-safe-component c) }}
      {{/let}}
    {{/if}}
  {{/let}}
{{/if}}
